procedure cuadro;
Var
    c1,f1,c2,f2 :integer;
Begin
f1:=2;
c2:=3;
    For c1:=3 to 37 Do
      begin
        //delay(50);
        gotoxy(c1,f1); write('อ');
        gotoxy(c1,f1+20); write('อ');
      end;
    For f2:=f1 to f1+20 Do
      begin
        gotoxy(c2,f2); write('บ');
        gotoxy(c1,f2); write('บ');
      end;
    gotoxy(c1,f1); write('ป');
    gotoxy(c1,f1+20); write('ผ');
    gotoxy(c2,f1); write('ษ');
    gotoxy(c2,f1+20); write('ศ');
    //Salida
//    gotoxy(c1,f1+8); write('ศอ');
    gotoxy(c1,f1+9); write('ศ');
    gotoxy(c1,f1+10); write(' => Salida =>');
    gotoxy(c1,f1+11); write('ษ');
end;

procedure figura(c,f :integer);
